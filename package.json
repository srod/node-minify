{
  "name": "node-minify",
  "version": "3.5.1",
  "description": "Javascript / CSS minifier based on Babel-minify / Butternut / YUI Compressor / Google Closure Compiler / Terser / UglifyJS2 / Sqwish / Clean-css / CSSO",
  "homepage": "https://github.com/srod/node-minify",
  "author": {
    "name": "Rodolphe Stoclin",
    "email": "rodolphe@2clics.net",
    "url": "http://2clics.net"
  },
  "license": "MIT",
  "main": "lib/node-minify.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/srod/node-minify.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "compressor",
    "minify",
    "minifier",
    "yui",
    "gcc",
    "google",
    "closure",
    "compiler",
    "uglifyjs",
    "uglifyjs2",
    "windows",
    "sqwish",
    "clean-css",
    "csso",
    "babel-minify",
    "butternut",
    "terser"
  ],
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "eslint": "eslint lib __tests__ || true",
    "pretest": "npm run eslint",
    "test": "jest",
    "posttest": "npm run clean",
    "clean": "rm -f ./examples/public/dist/* ./examples/public/js-dist/* ./examples/public/css-dist/*",
    "release-patch": "npm version patch -m 'Bump %s' && git push --tags origin HEAD:master",
    "release-minor": "npm version minor -m 'Bump %s' && git push --tags origin HEAD:master",
    "release-major": "npm version major -m 'Bump %s' && git push --tags origin HEAD:master",
    "release-next-major": "npm version premajor --preid=beta && git push --tags origin HEAD:next",
    "release-next-prerelease": "npm version prerelease --preid=beta && git push --tags origin HEAD:next",
    "showpack": "npm pack --dry",
    "publish-latest": "npm publish",
    "publish-next": "npm publish --tag next",
    "prettier": "prettier",
    "prettier-write": "npm run prettier -- --write",
    "prettier-project": "npm run prettier-write -- '{lib,__tests__}/**/*.js'"
  },
  "bin": {
    "node-minify": "bin/cli.js"
  },
  "dependencies": {
    "babel-core": "6.26.3",
    "babel-preset-minify": "0.5.0",
    "butternut": "0.4.6",
    "chalk": "2.4.1",
    "clean-css": "4.2.1",
    "commander": "2.19.0",
    "crass": "0.12.3",
    "csso": "3.5.1",
    "depd": "2.0.0",
    "glob": "7.1.3",
    "google-closure-compiler": "20181008.0.0",
    "gzip-size": "5.0.0",
    "html-minifier": "3.5.21",
    "mkdirp": "0.5.1",
    "node-version": "1.2.0",
    "ora": "3.0.0",
    "sqwish": "0.2.2",
    "terser": "3.10.4",
    "uglify-es": "3.3.9",
    "uglify-js": "3.4.9",
    "update-notifier": "2.5.0"
  },
  "devDependencies": {
    "babel-preset-env": "1.7.0",
    "codecov": "3.1.0",
    "eslint": "5.0.1",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-prettier": "3.0.0",
    "husky": "1.1.2",
    "jest": "23.1.0",
    "lint-staged": "8.0.2",
    "prettier": "1.14.3"
  },
  "jest": {
    "notify": true,
    "verbose": true,
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".history"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier-write --",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
