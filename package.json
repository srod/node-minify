{
  "name": "node-minify",
  "version": "2.0.0-beta.1",
  "description": "Javascript / CSS minifier based on YUI Compressor / Google Closure Compiler / UglifyJS2 / Sqwish / Clean-css / CSSO",
  "homepage": "https://github.com/srod/node-minify",
  "author": {
    "name": "Rodolphe Stoclin",
    "email": "rodolphe@2clics.net",
    "url": "http://2clics.net"
  },
  "license": "MIT",
  "main": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/srod/node-minify.git"
  },
  "engines": {
    "node": ">=0.12.0",
    "iojs": ">=1.0.0"
  },
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-preset-babili": "0.0.5",
    "bluebird": "^3.4.6",
    "clean-css": "^3.4.19",
    "csso": "^2.2.1",
    "depd": "^1.1.0",
    "glob": "^7.1.1",
    "google-closure-compiler-js": "^20160916.0.0",
    "mkdirp": "^0.5.1",
    "node-version": "^1.0.0",
    "sqwish": "^0.2.2",
    "uglify-js": "^2.7.3",
    "xtend": "^4.0.1"
  },
  "keywords": [
    "compressor",
    "minify",
    "minifier",
    "yui",
    "gcc",
    "google",
    "closure",
    "compiler",
    "uglifyjs",
    "uglifyjs2",
    "windows",
    "sqwish",
    "clean-css",
    "csso"
  ],
  "scripts": {
    "eslint": "eslint index.js lib test || true",
    "mocha": "mocha --bail --timeout 60000 --reporter spec || true",
    "pretest": "npm run eslint",
    "test-cov": "npm run pretest && istanbul cover ./node_modules/mocha/bin/_mocha -- -t 60000 -R spec",
    "test": "npm run mocha",
    "posttest": "npm run clean",
    "clean": "rm -f ./examples/public/dist/*",
    "clean-cov": "npm run clean && rm -Rf ./coverage",
    "release-patch": "npm version patch -m 'Bump %s'",
    "release-minor": "npm version minor -m 'Bump %s'",
    "release-major": "npm version major -m 'Bump %s'",
    "publish-latest": "git push --tags origin HEAD:master && npm publish",
    "publish-beta": "npm publish --tag beta"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "eslint": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "should": "^11.1.0",
    "sinon": "^1.17.5"
  }
}
