{
  "env": {
    "es6": true,
    "node": true,
    "jest": true,
    "jasmine": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "printWidth": 120
      }
    ]
  },
  "extends": "prettier"
}
